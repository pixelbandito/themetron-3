{
  "name": "@pixelbandito/themed-ui",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "pixelbandito",
  "license": "UNLICENSED",
  "repository": "pixelbandito/themed-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "deploy": "gh-pages -d example/build",
    "jest:screen": "jest -c __screen-tests__/jest.config.js",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepublish": "run-s build",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "storybook": "start-storybook -p 9009",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:screen": "start-server-and-test storybook http-get://localhost:9009 jest:screen",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "test": "run-s test:unit test:lint test:build"
  },
  "dependencies": {
    "@pixelbandito/theme": "^1.0.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-transition-group": "^4.3.0",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.19",
    "@testing-library/dom": "^7.21.5",
    "@testing-library/react": "~10.4.7",
    "@testing-library/user-event": "^12.0.15",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-image-snapshot": "^4.1.0",
    "jest-puppeteer": "^4.4.0",
    "microbundle-crl": "^0.13.10",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "start-server-and-test": "^1.11.2"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": ["./jest.setup.js"]
  }
}
